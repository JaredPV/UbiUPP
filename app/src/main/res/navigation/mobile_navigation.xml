<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_inicio">

    <fragment
        android:id="@+id/navigation_inicio"
        android:name="com.example.ubiupp.ui.home.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_mapa"
        android:name="com.example.ubiupp.ui.mapa.MapaFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_mapa" />

    <fragment
        android:id="@+id/nav_buscar"
        android:name="com.example.ubiupp.ui.buscar.BuscarFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_buscar" />
</navigation>